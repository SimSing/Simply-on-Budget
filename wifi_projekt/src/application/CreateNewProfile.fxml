<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" style="-fx-background-color: GAINSBORO;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CreateNewProfileController">
   <children>
      <Line endX="65.0" layoutX="130.0" layoutY="55.0" startX="-100.0" />
      <TextField fx:id="userTextField" layoutX="24.0" layoutY="29.0" prefHeight="25.0" prefWidth="178.0" promptText="UserName" style="-fx-background-color: GAINSBORO;" />
      <PasswordField fx:id="passwordPasswordField" layoutX="22.0" layoutY="83.0" prefHeight="25.0" prefWidth="178.0" promptText="Password" style="-fx-background-color: GAINSBORO;" />
      <PasswordField fx:id="confirmPasswordField" layoutX="22.0" layoutY="137.0" prefWidth="178.0" promptText="Confirm Password" style="-fx-background-color: GAINSBORO;" />
      <Button fx:id="createButton" layoutX="28.0" layoutY="201.0" mnemonicParsing="false" onAction="#onCreateButtonPressed" prefHeight="25.0" prefWidth="245.0" style="-fx-background-radius: 10;" text="Create" />
      <Button fx:id="cancelButton" layoutX="28.0" layoutY="241.0" mnemonicParsing="false" onAction="#onCancelButtonPressed" prefHeight="25.0" prefWidth="245.0" style="-fx-background-radius: 10;" text="Cancel" />
      <Line endX="65.0" layoutX="130.0" layoutY="108.0" startX="-100.0" />
      <Line endX="65.0" layoutX="128.0" layoutY="162.0" startX="-100.0" />
   </children>
   <effect>
      <Blend mode="MULTIPLY" />
   </effect>
</AnchorPane>
