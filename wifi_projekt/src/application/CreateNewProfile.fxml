<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="215.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CreateNewProfileController">
   <children>
      <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="15.0" prefHeight="45.0" prefWidth="300.0">
         <children>
            <Label text="User:">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="userTextField" prefWidth="100.0">
               <HBox.margin>
                  <Insets left="110.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="65.0" prefHeight="45.0" prefWidth="300.0">
         <children>
            <Label text="Password:">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
            <PasswordField fx:id="passwordPasswordField" prefWidth="100.0">
               <HBox.margin>
                  <Insets left="84.0" />
               </HBox.margin>
            </PasswordField>
         </children>
      </HBox>
      <Button fx:id="createButton" layoutX="54.0" layoutY="176.0" mnemonicParsing="false" onAction="#onCreateButtonPressed" prefWidth="52.0" text="Create" />
      <Button fx:id="cancelButton" layoutX="197.0" layoutY="176.0" mnemonicParsing="false" onAction="#onCancelButtonPressed" text="Cancel" />
      <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="115.0" prefHeight="45.0" prefWidth="300.0">
         <children>
            <Label text="Confirm Password:">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
            <PasswordField fx:id="confirmPasswordField" prefWidth="100.0">
               <HBox.margin>
                  <Insets left="38.0" />
               </HBox.margin>
            </PasswordField>
         </children>
      </HBox>
   </children>
</AnchorPane>
